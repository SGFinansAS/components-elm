-- Icon Wrappers


leftIcon : Html msg -> Html msg
leftIcon icon =
    Html.div [ css [ displayFlex, marginRight (rem 0.28125) ] ]
        [ icon
        ]


rightIcon : Html msg -> Html msg
rightIcon icon =
    Html.div [ css [ displayFlex, marginLeft (rem 0.28125) ] ]
        [ icon
        ]



